[color]
	branch = auto
	diff = auto
	interactive = auto
	status = auto
[alias]
	st = status -sb
	unstage = reset
	unchange = checkout --
    co = checkout
	slog = log --pretty=oneline --abbrev-commit --decorate -10
	aa = add --all
    glog = log --all -15 --graph --pretty=oneline --abbrev-commit --decorate
    dc = diff --cached
[core]
	editor = /usr/bin/vim
	excludesfile = /Users/nation/.gitignore
