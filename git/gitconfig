[color]
	branch = auto
	diff = auto
	interactive = auto
	status = auto
[alias]
	st = status -sb
	unstage = reset
	unchange = checkout --
    co = checkout
    mylog = log --graph --abbrev-commit --date=relative --all
    l = "!source ~/.githelpers && pretty_git_log"
    sl = "!git l -15"
    sla = "!git l --all -15"
	aa = add --all
    glog = log --all -15 --graph --pretty=oneline --abbrev-commit --decorate
    dc = diff --cached
    cam = commit -am
    cob = checkout -b
	df = diff --word-diff
    pom = push origin master
[core]
	editor = /usr/bin/vim
	excludesfile = /Users/nation/.gitignore
[format]
    pretty=format:%C(yellow)%h%Creset -%C(blue)%d%Creset %s %Cgreen(%ar)
[clean]
	requireForce = false
