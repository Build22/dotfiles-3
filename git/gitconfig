[color]
	branch = auto
	diff = auto
	interactive = auto
	status = auto
[alias]
	st = status -sb
	unstage = reset
	unchange = checkout --
  b = branch
  co = checkout
  mylog = log --graph --abbrev-commit --date=relative --all
  l = "!source ~/.githelpers && pretty_git_log"
  sl = "!git l -15"
  sla = "!git l --all -15"
  unreleased = "!git l production/master..master"
	aa = add --all
  glog = log --all -15 --graph --pretty=oneline --abbrev-commit --decorate
  dc = diff --cached
  cm = commit -m
  cam = commit -am
  cob = checkout -b
	df = diff --word-diff
  rom = remote add origin
  pom = push origin master
  bom = branch --set-upstream master origin/master
  copysha = "!git rev-parse HEAD | pbcopy"
[core]
	editor = /usr/bin/vim
	excludesfile = /Users/nation/.gitignore
[clean]
	requireForce = false
[url "git@heroku.com:"]
  # $ git remote add heroku:sushi.git
  insteadOf = heroku:
[url "git://github.com/"]
  # Read-only
  insteadOf = gh:
[url "git@github.com:"]
  # With write access
  insteadOf = wgh:
